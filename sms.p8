	 }
	debug = ""
	flipped = false
	
	sel_item=0
	gamestate=""
	held_item=nil
	held_w=1
	held_h=1
end

function _update()
	if gamestate=="menu" then
		if (btnp(â¬†ï¸)) advance_menu(-1)
		if (btnp(â¬‡ï¸)) advance_menu(1)
		if (btnp(âŽ)) select_menu()
		if (btnp(ðŸ…¾ï¸)) gamestate=""
		do return end
	end

 dir = {x=0, y=0}
 if (btn(â¬…ï¸)) dir.x-=1
 if (btn(âž¡ï¸)) dir.x+=1
 if (btn(â¬†ï¸)) dir.y-=1
 if (btn(â¬‡ï¸)) dir.y+=1
 if (btn(âŽ)) gamestate="menu"
 
 if (dir.x!=0 or dir.y!=0) and
 	  is_moving_allowed(dir) then
  	x += dir.x
			y += dir.y
			if (dir.x!=0) flipped=(dir.x == -1)
  	advance_anim(legs_anim)
  	advance_anim(player_anim)
 else
  stand_still()
 end
 --debug=stat(1)
end

function is_moving_allowed(dir)
	local solid = is_solid(x+dir.x+6,y+dir.y+15) or
	is_solid(x+dir.x+6,y+dir.y+9) or
	is_solid(x+dir.x+2,y+dir.y+15) or
	is_solid(x+dir.x+2,y+dir.y+9)
	return not solid
	       
end

function is_solid(abs_x,abs_y)
	sprt1=mget(abs_x/8,abs_y/8)
	sprt2=mget(abs_x/8+16,abs_y/8)
 return fget(sprt1,0) or
 						 fget(sprt2,0)
end

function advance_anim(anim)
	if flr(anim.curr)==anim.to then
		anim.curr=anim.from
	else
	 anim.curr+=0.26
	end
end

function stand_still()
	legs_anim.curr=legs_anim.from
	player_anim.curr=player_anim.from
end

function _draw()
 cls()
	map(0,0) --floors
	map(16,0,0,0,16,16) --above
 spr(player_anim.curr,x,y,1,1,flipped)
	spr(legs_anim.curr,x,y+8,1,1,flipped)
	if (held_item) draw_held()
	map(16,0,0,0,16,16,0x2) --below
 print(debug,0,0,8)
 
 if gamestate=="menu" then
 	rectfill(2,106,40,126,1)
 	rectfill(2,108+sel_item*6,
 										40,112+sel_item*6,12)
 	print("pick",4,108,7)
 	print("use",4,114,7)
 end
end
function mget_scr(x,y)
 return	mget(x/8,y/8)
end

function draw_held()
	local draw_x=1
	if (flipped) draw_x=-1

	draw_x *= (held_w+1)
	
	spr(held_item,x+draw_x,y,held_h,held_h)
end

function mset_scr(x,y,val)
	mset(x/8,y/8,val)
end

function mget_obj(x,y)
	return mget_scr(x+128,y)
end

function mset_obj(x,y,val)
	mset_scr(x+128,y,val)
end

function fcheck(flags, value)
	return flr(band(flags,value))==value
end

--right,left,up,down
function seek_first(spr)
	local flags=fget(spr)
	if fcheck(flags,0x40) then --uo
		return seek_first(spr-16)
	elseif fcheck(flags,0x20) then --left
		return seek_first(spr-1)
	elseif fcheck(flags,0x80) or --down
		fcheck(flags,0x10) or --right
		fcheck(flags,0x90) then
			return spr
	else
		return -1
	end
end

function seek_last(spr)
	local flags=fget(spr)
	if fcheck(flags,0x80) then
		return seek_last(spr+16)
	elseif fcheck(flags,0x10)then
		return seek_last(spr+1)
	elseif fcheck(flags,0x20)
		or fcheck(flags,0x40)
		or fcheck(flags,0x60) then
			return spr
	else
		return -1
	end
end

function pick_obj(x,y)
	local picked_sprite=mget_obj(x,y)
	first_spr=seek_first(picked_sprite)
	last_spr=seek_last(picked_sprite)
	held_item=first_spr
	held_w = (last_spr-first_spr)%16+1
	held_h = flr((last_spr-first_spr)/16)+1
	mset_obj(x,y,0)
end

function pick_item()
	local dir_x=0
	if flipped then
		dir_x=x-4
	else
		dir_x=x+12
	end	
	local dir_y = y + 6
	pick_obj(dir_x,dir_y)
end




