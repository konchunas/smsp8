pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
 x=64
 y=64
 legs_anim = {
 	from=17, to=21, curr=17
 	}
 player_anim = {
 	from=1, to=4, curr=1
	 }
	debug = ""
	flipped = false	
end

function _update()
 dir = {x=0, y=0}
 if (btn(⬅️)) dir.x-=1
 if (btn(➡️)) dir.x+=1
 if (btn(⬆️)) dir.y-=1
 if (btn(⬇️)) dir.y+=1
  
 if (dir.x!=0 or dir.y!=0) and
 	  is_moving_allowed(dir) then
  	x += dir.x
			y += dir.y
			if (dir.x!=0) flipped=(dir.x == -1)
  	advance_anim(legs_anim)
  	advance_anim(player_anim)
 else
  stand_still()
 end
end

function is_moving_allowed(dir)
	local solid = is_solid(x+dir.x+6,y+dir.y+15) or
	is_solid(x+dir.x+6,y+dir.y+9) or
	is_solid(x+dir.x+2,y+dir.y+15) or
	is_solid(x+dir.x+2,y+dir.y+9)
	return not solid
	       
end

function is_solid(abs_x,abs_y)
	sprt=mget(abs_x/8,abs_y/8)
 return fget(sprt,0)
end

function advance_anim(anim)
	if flr(anim.curr)==anim.to then
		anim.curr=anim.from
	else
	 anim.curr+=0.26
	end
end

function stand_still()
	legs_anim.curr=legs_anim.from
	player_anim.curr=player_anim.from
end

function _draw()
 cls()
	map(0,0)
 spr(player_anim.curr,x,y,1,1,flipped)
	spr(legs_anim.curr,x,y+8,1,1,flipped)
	map(0,0,0,0,16,16,0x2)
 print(debug,0,0,8)
end





